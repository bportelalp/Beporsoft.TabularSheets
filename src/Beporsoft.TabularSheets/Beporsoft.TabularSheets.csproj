<Project Sdk="Microsoft.NET.Sdk">

	<PropertyGroup>
		<TargetFramework>net6.0</TargetFramework>
		<ImplicitUsings>enable</ImplicitUsings>
		<Nullable>enable</Nullable>
		<Title>Tabular sheets</Title>
		<Copyright>MIT</Copyright>
		<PackageProjectUrl>https://github.com/bportelalp/tabular-data-sheets</PackageProjectUrl>
		<PackageReadmeFile>README.md</PackageReadmeFile>
		<RepositoryUrl>https://github.com/bportelalp/tabular-data-sheets</RepositoryUrl>
		<GeneratePackageOnBuild>False</GeneratePackageOnBuild>
		<Authors>bportelalp</Authors>
	</PropertyGroup>

	<ItemGroup>
		<None Include="..\..\README.md">
			<Pack>True</Pack>
			<PackagePath>\</PackagePath>
		</None>
	</ItemGroup>

	<ItemGroup>
		<AssemblyAttribute Include="System.Runtime.CompilerServices.InternalsVisibleTo">
			<_Parameter1>Beporsoft.TabularSheets.Test</_Parameter1>
		</AssemblyAttribute>
	</ItemGroup>


	<ItemGroup>
		<PackageReference Include="DocumentFormat.OpenXml" Version="2.19.0" />
	</ItemGroup>

</Project>
